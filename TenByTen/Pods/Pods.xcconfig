PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AFNetworking" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/Nimbus" "${PODS_ROOT}/Headers/TestFlightSDK"
ALWAYS_SEARCH_USER_PATHS = YES